# Docker 容器端口配置
# 注意：這些端口配置用於 docker-compose.yml

# 前端端口
FRONTEND_PORT=3201

# 後端端口
BACKEND_PORT=9218

# MySQL 端口
MYSQL_PORT=9318
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=esg_db
MYSQL_USER=esg_user
MYSQL_PASSWORD=esg_pass

# phpMyAdmin 端口
PHPMYADMIN_PORT=9418

# 環境
NODE_ENV=development
CI_ENVIRONMENT=development