# Authentication Settings
# Set to 'true' to require login, 'false' to bypass authentication
NUXT_PUBLIC_REQUIRE_AUTH=false

# UI Settings
# Set to 'true' to show footer, 'false' to hide footer
NUXT_PUBLIC_SHOW_FOOTER=false

# Application Settings
# Application title shown in the navbar
NUXT_PUBLIC_APP_TITLE=ESGMATE

# Backend API Configuration
BACKEND_API_URL=http://localhost:9218/api/v1
BACKEND_HOST=localhost
BACKEND_PORT=9218

# Nuxt Configuration
NUXT_PUBLIC_API_BASE_URL=http://localhost:9218/api/v1
NUXT_PUBLIC_BACKEND_URL=http://localhost:9218/api/v1
NUXT_PUBLIC_BACKEND_PORT=9218